{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["react-app"],
  "rules": {
    "no-console": "warn",
    "import/no-relative-packages": "error"
  },
  "overrides": [
    {
      "files": ["packages/**"],
      "excludedFiles": "*.stories.*",
      "rules": {
        "import/no-extraneous-dependencies": ["error"]
      }
    }
  ]
}
